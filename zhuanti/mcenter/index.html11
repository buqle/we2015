                        $('#zcnum').text(r.data.num);


                        $('#first_recharge_time').text(r.data.first_recharge_time);
                        $('#first_recharge_account').text(r.data.first_recharge_account);

                        $('#first_recharge_time').text(r.data.first_recharge_time);
                        $('#first_recharge_account').text(r.data.first_recharge_account);

                        $('#first_tendtime').text(r.data.first_tendtime);
                        $('#self_alltned').text(r.data.self_alltned);
                        $('#all_account').text(r.data.all_account);
                        $('#first_account').text(r.data.first_account);

                        $('#first_interest_time').text(r.data.first_interest_time);
                        $('#first_interest_yes').text(r.data.first_interest_yes);
						 $('#self_all_interest').text(r.data.self_all_interest);
                        $('#all_account_yes_sum').text(r.data.all_account_yes_sum);

                        $('#friendname').text(r.data.friendname);
                        $('#friend_reg').text(r.data.friend_reg);

                        $('#allday').text(r.data.allday);
                        $('#first_interest_yes').text(r.data.first_interest_yes);


                    }
                    else
                        alert("系统忙，请稍后再试！");
                },
                error: function () {
                }
            });
        }

    }
</script>
